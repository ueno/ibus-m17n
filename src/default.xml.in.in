		<preedit-highlight>TRUE</preedit-highlight>
	</engine>
	<engine>
		<name>m17n:zh:quick</name>
		<preedit-highlight>TRUE</preedit-highlight>
	</engine>
	<engine>
		<name>m17n:zh:tonepy-b5</name>
		<preedit-highlight>TRUE</preedit-highlight>
	</engine>
	<engine>
		<name>m17n:zh:tonepy-gb</name>
		<preedit-highlight>TRUE</preedit-highlight>
	</engine>
	<engine>
		<name>m17n:zh:tonepy</name>
		<preedit-highlight>TRUE</preedit-highlight>
	</engine>
	<engine>
		<name>m17n:ja:anthy</name>
		<preedit-highlight>TRUE</preedit-highlight>
	</engine>
</engines>
